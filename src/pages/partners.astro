---
import "@/styles/global.css";
import "@/styles/partners.css";
import Navigation from "@/components/Navigation.astro";
import Footer from "@/components/Footer.astro";
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <meta name="viewport" content="width=device-width" />
    <meta name="description" content="QUT AI & ML Society Sponsors Page" />
    <title>QUT AI & ML Society</title>
  </head>

  <body class="background-dots">
    <Navigation />

    <div class="content-wrapper top-space vacant-space">
      <!-- ABOUT -->
      <section class="pink-panel">
        <p class="pink-title">Our Partners</p>
      </section>

		<div class="info-panel-wrapper">
			<div class="thick-line-behind"></div>			
			<section class="info-panel">
                <p>We're currently building our partners page!</p>
                <p>Stay tuned to see the amazing organisations sponsoring QUT AI & ML Society.</p>
            </section>
        </div>
    </div>

    <Footer />
  </body>
</html>
